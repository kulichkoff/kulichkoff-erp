  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "start:admin": "nx run admin:serve:development",
    "start:api": "nx run api:serve",
    "build": "yarn run clear:dist && nx run-many --target=build --all && nx run-many --target=server --all",
    "test": "nx run-many --target=test --all",
    "clear:dist": "rimraf dist",
    "deploy": "docker-compose --context remote_kulichkoff build && docker-compose --context remote_kulichkoff up -d"
  },
